# Java 项目 .gitignore 文件

# 编译输出
target/
build/
out/

# 依赖目录
lib/

# Maven 相关
.mvn/
*.jar
*.war
*.nar
*.ear
*.zip
*.tar.gz
*.rar

# Gradle 相关（如果使用）
.gradle/
gradle-app.setting

# 日志文件
logs/
*.log
logback.xml

# IDEA 项目文件
*.iml
*.ipr
*.iws
.idea/

# Eclipse 项目文件
.classpath
.project
.settings/

# 操作系统生成的文件
.DS_Store
Thumbs.db

# 临时文件
*.tmp
*.temp
*.class
*.o
*.obj

# 环境变量文件
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Spring Boot 配置文件（可选，根据需要保留）
# application.properties
# application.yml
# application-*.yml

# 数据库文件
*.db
*.sqlite
*.sqlite3

# 测试覆盖相关
coverage/
*.lcov

# 其他临时文件
*~
.fuse_hidden*
.directory
.Trash-*
.nfs*

# 备份文件
*.bak
*.old
*.backup

# 系统文件
Thumbs.db
ehthumbs.db
Desktop.ini

# 版本控制信息
.svn/
.git/
.hg/

# 脚本文件（如果不希望提交）
# *.sh
# *.bat
# *.ps1